{
  "name": "ember-materialize-shim",
  "version": "0.5.1",
  "description": "A simple shim for using Materialize in Ember.js apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember server",
    "test": "ember try:testall"
  },
  "repository": "https://github.com/levanto-financial/ember-materialize-shim",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "2.18.0",
    "ember-cli-dependency-checker": "3.0.0",
    "ember-cli-eslint": "4.2.3",
    "ember-cli-fastboot": "1.1.1",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "2.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-disable-proxy-controllers": "1.0.1",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "1.1.0",
    "ember-material-design-icons-shim": "0.1.12",
    "ember-resolver": "4.5.6",
    "ember-source": "3.0.0-beta.4",
    "ember-sublime": "0.0.9",
    "eslint-plugin-ember": "5.2.0",
    "eslint-plugin-node": "8.0.0",
    "loader.js": "4.7.0",
    "node-sass": "4.7.2"
  },
  "keywords": [
    "ember-addon",
    "material",
    "materialdesign",
    "materialize",
    "ui",
    "form",
    "skin",
    "paper",
    "bootstrap",
    "design",
    "android"
  ],
  "dependencies": {
    "broccoli-funnel": "1.2.0",
    "broccoli-merge-trees": "3.0.1",
    "ember-cli-babel": "7.1.3",
    "ember-cli-htmlbars": "3.0.1",
    "ember-cli-sass": "7.1.3",
    "exists-sync": "0.0.4",
    "fastboot-transform": "0.1.3"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
